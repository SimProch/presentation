{
  "homepage": "http://simproch.github.io/presentation",
  "name": "presentation",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "node-sass": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "lint": "eslint src/**/*.js --max-warnings 5",
    "cypress:run": "cypress run --record --key=${CYPRESS_RECORD_KEY}",
    "ci:cypress-run": "start-server-and-test start http://localhost:3000 cypress:run",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@csstools/normalize.css": "^10.1.0",
    "cypress": "^3.6.0",
    "eslint": "^6.6.0",
    "gh-pages": "^2.1.1",
    "start-server-and-test": "^1.10.6"
  }
}